<?xml version="1.0" encoding="utf-8"?>
<GatherRoot>
    <sfdatasks name="">
        <!-- 任务的名称　-->
        <taskName></taskName>
        <!-- 最后一次更新的ID-->
        <lastUpdateID></lastUpdateID>
        <!-- 数据库同步的量-->
        <syncData>2000</syncData>
        <!-- 是否自动爬取 -->
        <ISAutoFetch>-1</ISAutoFetch>
        <!-- 间隔时间 -->
        <IntervalTime></IntervalTime>
        <!-- 全部更新-->
        <allUpdate>-1</allUpdate>
        <!-- 出错的数据ID -->
        <errorDataID></errorDataID>
        <!-- 爬取数据到数据库的表名 -->
        <Datatable>t_drugnewsinfo</Datatable>
        <!-- 附件存放位置 -->
        <attachposition></attachposition>
        <webGather>
            <!-- 规则名称 -->
            <GatherName>cfdaRule</GatherName>
            <!-- 站点域名 -->
            <SiteDomain>http://app1.sfda.gov.cn/datasearch/face3/</SiteDomain>
            <!-- 描述 -->
            <describe>cfda</describe>
            <!-- 网页字符集 -->
            <characterset>GB2312</characterset>
            <!-- 专业数据库ID -->
            <tableId>84</tableId>
            <!-- 开始ID -->
            <startID>1</startID>
            <!-- 结束ID -->
            <EndID>500</EndID>
            <!-- 批量更新数据库的记录 -->
            <batchUpdate>5</batchUpdate>
            <MappingXML>conf/DefaultRule.xml</MappingXML>
        </webGather>
        <!-- 映射的实体名 -->
        <databases>
            <choice>drugnewsinfo</choice>
            <drugnewsinfo>
                <mapper name="drugnewsinfo" class="com.skysoft.domain.drugnewsinfo">
                    <property name="artTitle" StartRegExp="${artTitle.startregexp}"
                              EndRegExp="${artTitle.endregexp}"/>
                    <property name="keywordes" StartRegExp="${keywordes.startregexp}"
                              EndRegExp="${keywordes.endregexp}"/>
                    <property name="newsTime" StartRegExp="${newsTime.startregexp}"
                              EndRegExp="${newsTime.endregexp}"/>
                    <property name="artSource" StartRegExp="${artSource.startregexp}"
                              EndRegExp="${artSource.endregexp}"/>
                    <property name="artSummary" StartRegExp="${artSummary.startregexp}"
                              EndRegExp="${artSummary.endregexp}"/>
                    <property name="createTime" StartRegExp="${createTime.startregexp}"
                              EndRegExp="${createTime.endregexp}"/>
                    <property name="sourceUrl" StartRegExp="${sourceUrl.startregexp}"
                              EndRegExp="${sourceUrl.endregexp}"/>
                    <property name="artColumn" StartRegExp="${artColumn.startregexp}"
                              EndRegExp="${artColumn.endregexp}"/>
                    <property name="artContent" StartRegExp="${artContent.startregexp}"
                              EndRegExp="${artContent.endregexp}"/>
                    <property name="artFlag" StartRegExp="${artFlag.startregexp}"
                              EndRegExp="${artFlag.endregexp}"/>
                </mapper>
            </drugnewsinfo>
        </databases>
    </sfdatasks>
    <sfdatasks name="">

    </sfdatasks>
</GatherRoot>
